{
  "name": "spartan-stats",
  "version": "1.0.0",
  "description": "Track your teams basketball stats and have a real time view of games",
  "scripts": {
    "build": "yarn vite build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --check .",
    "dev": "yarn vite & wait",
    "dev:host": "yarn vite --host & wait",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "rm -rf dist && yarn vite build && firebase deploy --only hosting"
  },
  "keywords": [
    "Basketball, Stats"
  ],
  "author": "Sandy McFadden",
  "license": "MIT",
  "dependencies": {
    "@date-io/dayjs": "2.11.0",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@mui/icons-material": "5.2.4",
    "@mui/lab": "5.0.0-alpha.62",
    "@mui/material": "5.2.4",
    "dayjs": "1.10.7",
    "firebase": "9.13.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "uuid": "8.3.2",
    "wouter": "2.8.0-alpha.2"
  },
  "devDependencies": {
    "@types/node": "16.11.13",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "@vitejs/plugin-react": "2.2.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.7.1",
    "typescript": "4.8.4",
    "vite": "3.1.0",
    "vite-plugin-pwa": "0.13.2"
  }
}
